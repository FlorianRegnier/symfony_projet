{% extends "front/base.html.twig" %}


{% block main %}

    

    <h1>Liste des licences</h1>

    <div class="licences">
        {% for licence in licences %}
        <div class="licence-card">
        
            <a href="{{ path('front_show_licence', { 'id' : licence.id }) }}" >
                <h2  > {{ licence.name }} </h2>
            </a>
            <img src="{{ asset('media/' ~ licence.media ) }}" alt="img">
            <p>  {{ licence.description }} </p>



            

        </div>
        {% endfor %}
    
    
    </div>


{% endblock %}